<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hta2405.unite.mybatis.mapper.CertMapper">
    <select id="getCertByEmpId" parameterType="String">
        SELECT *
        FROM cert
        WHERE emp_id = #{empId};
    </select>
</mapper>

