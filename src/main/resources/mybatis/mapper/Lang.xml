<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hta2405.unite.mybatis.mapper.LangMapper">
    <select id="getLangByEmpId" parameterType="String">
        SELECT *
        FROM lang
        WHERE emp_id = #{empId};
    </select>
</mapper>

